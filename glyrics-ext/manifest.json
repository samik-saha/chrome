{
  "manifest_version": 2,
  "key": "",
  "name": "Gaana Lyrics Extension",
  "short_name": "GLyrics Extension",
  "description": "Displays lyrics on Gaana.com, Google Music, Spotify, Pandora, Saavn, SoundCloud, Plex and other streaming music services",
  "version": "3.5.3",
  "author": "Samik",
  "icons": {
    "64": "images/icon64.png",
    "128": "images/icon128.png"
  },
  "options_page": "options/options.html",
  "background": {
    "scripts": [
      "jquery-2.0.3.min.js",
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "Show lyrics"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["http://gaana.com/*"],
      "css": [
        "css/lyrics.css",
        "css/jquery-ui.min.css"
      ],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery-ui.min.js",
        "script.js",
        "gaana/extract_track_info.js",
        "gaana/modify_page.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://www.accuradio.com/*"
      ],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery-ui.min.js",
        "script.js",
        "AccuRadio/extract_track_info.js"
      ],
      "run_at": "document_end",
      "css": [
        "css/lyrics.css",
        "css/jquery-ui.min.css"
      ]
    },
    {
      "matches": ["http://www.saavn.com/*"],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery-ui.min.js",
        "script.js",
        "saavn/extract_track_info.js"
      ],
      "run_at": "document_end",
      "css": [
        "css/lyrics.css",
        "css/jquery-ui.min.css"
      ]
    },
    {
      "matches": ["https://play.spotify.com/*"],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery-ui.min.js",
        "script.js",
        "spotify/extract_track_info.js"
      ],
      "run_at": "document_end",
      "css": [
        "css/lyrics.css",
        "css/jquery-ui.min.css"
      ]
    },
    {
      "matches": ["http://play.raaga.com/*"],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery-ui.min.js",
        "script.js",
        "raaga/extract_track_info.js"
      ],
      "run_at": "document_end",
      "css": [
        "css/lyrics.css",
        "css/jquery-ui.min.css"
      ]
    },
    {
      "matches": ["https://bop.fm/*"],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery-ui.min.js",
        "script.js",
        "bop.fm/extract_track_info.js"
      ],
      "run_at": "document_end",
      "css": [
        "css/lyrics.css",
        "css/jquery-ui.min.css"
      ]
    },
    {
      "matches": ["https://soundcloud.com/*"],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery-ui.min.js",
        "script.js",
        "soundcloud/extract_track_info.js"
      ],
      "run_at": "document_end",
      "css": [
        "css/lyrics.css",
        "css/jquery-ui.min.css"
      ]
    },
    {
      "matches": ["https://www.amazon.com/gp/dmusic/*"],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery-ui.min.js",
        "script.js",
        "amazonmusic/extract_track_info.js"
      ],
      "run_at": "document_end",
      "css": [
        "css/lyrics.css",
        "css/jquery-ui.min.css"
      ]
    },
    {
      "matches": ["https://play.google.com/music/*"],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery-ui.min.js",
        "script.js",
        "googlemusic/extract_track_info.js"
      ],
      "run_at": "document_end",
      "css": [
        "css/lyrics.css",
        "css/jquery-ui.min.css"
      ]
    },
    {
      "matches": ["http://www.earbits.com/*"],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery-ui.min.js",
        "script.js",
        "earbits/extract_track_info.js"
      ],
      "run_at": "document_end",
      "css": [
        "css/lyrics.css",
        "css/jquery-ui.min.css"
      ]
    },
    {
      "matches": ["http://www.pandora.com/*"],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery-ui.min.js",
        "script.js",
        "pandora/extract_track_info.js"
      ],
      "run_at": "document_end",
      "css": [
        "css/lyrics.css",
        "css/jquery-ui.min.css"
      ]
    },
    {
      "matches": ["http://www.rdio.com/*"],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery-ui.min.js",
        "script.js",
        "rdio/extract_track_info.js"
      ],
      "run_at": "document_end",
      "css": [
        "css/lyrics.css",
        "css/jquery-ui.min.css"
      ]
    },
    {
      "matches": [
        "http://plex.tv/web/*",
        "http://127.0.0.1:32400/*",
        "http://localhost:32400/*"
      ],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery-ui.min.js",
        "script.js",
        "plex/extract_track_info.js"
      ],
      "run_at": "document_end",
      "css": [
        "css/lyrics.css",
        "css/jquery-ui.min.css"
      ]
    },
    {
      "matches": [
        "http://app.rhapsody.com/*"
      ],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery-ui.min.js",
        "script.js",
        "rhapsody/extract_track_info.js"
      ],
      "run_at": "document_end",
      "css": [
        "css/lyrics.css",
        "css/jquery-ui.min.css"
      ]
    },
    {
      "matches": [
        "http://www.slacker.com/*"
      ],
      "js": [
        "jquery-2.0.3.min.js",
        "jquery-ui.min.js",
        "script.js",
        "slacker/extract_track_info.js"
      ],
      "run_at": "document_end",
      "css": [
        "css/lyrics.css",
        "css/jquery-ui.min.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "jquery-2.0.3.min.map",
    "images/*.png",
    "css/*.css",
    "fonts/*",
    "options/options.html",
    "gaana/gaana.css",
    "spotify/spotify.css",
    "pandora/pandora.css",
    "amazonmusic/amazonmusic.css",
    "soundcloud/soundcloud.css",
    "saavn/saavn.css",
    "earbits/earbits.css",
    "bop.fm/bop.fm.css",
    "googlemusic/googlemusic.css",
    "raaga/raaga.css",
    "rdio/rdio.css",
    "plex/plex.css",
    "rhapsody/rhapsody.css",
    "AccuRadio/AccuRadio.css",
    "slacker/slacker.css"
  ],
  "content_security_policy": "script-src 'self' https://clients5.google.com https://chrome.google.com https://apis.google.com https://platform.twitter.com https://ssl.google-analytics.com https://ajax.googleapis.com; frame-src 'self' https://clients5.google.com https://plusone.google.com https://www.facebook.com https://platform.twitter.com; object-src 'self'"
}
